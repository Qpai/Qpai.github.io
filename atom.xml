<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>清凉派</title>
 <link href="http://qpai.github.io/atom.xml" rel="self"/>
 <link href="http://qpai.github.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://qpai.github.io/</id>
 <author>
   <name>清凉派</name>
   <email>qingliangpai</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://qpai.github.io/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://qpai.github.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>